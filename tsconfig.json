{
    "compilerOptions": {
        "target": "ESNext",
        "moduleResolution": "Node",
        "lib": [
            "ESNext",
        ],
        "noEmit": true,
        "pretty": true,
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "strict": true,
        "alwaysStrict": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "strictBindCallApply": true,
        "strictPropertyInitialization": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
    }
}